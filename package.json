        "typescript": "^4.7.4"
    },
    "react-native": "src/index.ts",
    "source": "src/index.ts",
    "module": "dist/module/index.js",
    "typings": "./dist/typescript/index.d.ts",
    "react-native-builder-bob": {
        "source": "src",
        "output": "dist",
        "targets": [
            "commonjs",
            "module",
            "typescript"
        ]
